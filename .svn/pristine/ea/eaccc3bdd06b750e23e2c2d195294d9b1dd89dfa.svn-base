    padding-left: 45px
  .information
    background: url(/assets/flashicons/information_small.png) 18px 20px  no-repeat
    min-height: 30px
    padding-left: 45px
  .warning
    background: url(/assets/flashicons/warning_small.png) 18px 20px no-repeat
    min-height: 30px
    padding-left: 45px
  .error
    background: url(/assets/flashicons/error_small.png) 18px 20px  no-repeat
    min-height: 30px
    padding-left: 45px
  .fatal
    background: url(/assets/flashicons/fatal_small.png) 18px 20px  no-repeat
    min-height: 30px
    padding-left: 45px

