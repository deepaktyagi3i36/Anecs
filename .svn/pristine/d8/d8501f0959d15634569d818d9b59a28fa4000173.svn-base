<%= form_for :feedback_filter, :url => {:action => "index"},
:html => {:method => :get, :id => "feedbacks_filter", :class => "list-filter" } do |f| %>
  <%= f.error_messages %>
  <fieldset>
    <ul>
      <li>
        <%= f.label :contact_type_id %>
        <%= f.collection_select :contact_type_id, @contact_types_filter, :id, :name %>
        <%= f.error_message_on :contact_type_id %>
      </li>
      <li>
        <%= f.label :contact_status_id %>
        <%= f.collection_select :contact_status_id, @contact_statuses_filter, :id, :name %>
        <%= f.error_message_on :contact_status_id %>
      </li>
      <li>
        <%= f.label :title %>
        <%= f.text_field :title %>
        <%= f.error_message_on :title %>
      </li>
      <li>
        <%= f.label :created_at_since %>
        <%= f.date_time_picker :created_at_since %>
        <%= f.error_message_on :created_at_since %>
      </li>
      <li>
        <%= f.label :created_at_till %>
        <%= f.date_time_picker :created_at_till %>
        <%= f.error_message_on :create_at_after %>
      </li>
    </ul>
    <%= f.submit t("buttons.filtrate") %>
  </fieldset>
<% end %>
<%= set_focus_on_load("feedback_filter_contact_type_id") %>

