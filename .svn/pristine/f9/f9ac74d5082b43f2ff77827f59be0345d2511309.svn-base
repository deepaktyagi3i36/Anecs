input[type="submit"].red-button, .red-button
  background: url(/assets/wellness/images/primitives/btn_bg_red.png) top repeat-x #6b0118
  color: #ededed
  cursor: pointer


input[type="submit"].disabled-button, .disabled-button
  background: url(/assets/wellness/images/primitives/btn_bg_grey.png) top repeat-x
  color: #777
  cursor: default


/* =Table of contents styles
 * This section contains table of contents styles for all site. These styles
 * used for all pages with tabe of content, for example: library or manpages.
 */
.table-of-contents h1
  margin-top: 0px


.table-of-contents h2 a
  font-size: 14px
  color: #093668
  border-bottom: 0px solid #093668


.table-of-contents h2 a:hover
  border-bottom: 1px dotted #093668


/* =Common preformed styles
 * This section contains preformed styles for all site. These styles used to
 * show content that was created by tinymce editor. The apperance of these
 * content should be the same at tiny mce editor and on the page.
 */
.tiny_mce
  background: white
  margin-top: 10px


.tiny_mce h2
  margin-left: 0px
  margin-bottom: 5px
  margin-left: 14px


.tiny_mce h3
  font-weight: bold
  color: #0b0b0b
  margin-left: 14px


.tiny_mce h4
  font-weight: bold
  color: #0b0b0b
  font-style: italic
  margin-left: 14px


.tiny_mce p
  text-indent: 12px


.tiny_mce ul
  margin-left: 12px


.tiny_mce ul li
  background: url(/assets/wellness/images/decorators/square.png) left top no-repeat
  padding-left: 15px


/* =Common html text styles
 * This section contains html text styles for all site. These styles used to
 * show static content.
 */
.html-text
  background: white
  margin-top: 10px


.html-text h1
  text-indent: 12px
  color: #a40304
  font-weight: normal
  font-size: 22px


.html-text h2
  text-indent: 12px
  font-size: 14px
  color: black
  font-weight: normal
  margin: 5px 0


.html-text p
  text-indent: 12px


.html-text p strong
  text-indent: 12px
  font-weight: bold


.html-text ul
  margin-left: 12px


.html-text ul li
  background: url(/assets/wellness/images/decorators/square.png) left top no-repeat
  padding-left: 15px



/* =Progress bar styles
 */
.progress-bar
  overflow: hidden


.progress-bar>ul
  color: #0b0b0b
  font-size: 12px
  background: url(/assets/wellness/images/progressbar/bg.png) repeat-x
  height: 20px
  margin: 10px auto
  float: left
  clear: both


.progress-bar>ul>li
  display: inline
  float: left
  background: none !important
  height: 20px
  padding-left: 15px


.progress-bar>ul>li:first-child
  background: url(/assets/wellness/images/progressbar/left.png) left top no-repeat !important
  transparent: !important


.progress-bar>ul>li.selected
  background: url(/assets/wellness/images/progressbar/hover_left.png) left top no-repeat !important
  display: block


.progress-bar a, .progress-bar div
  background: url(/assets/wellness/images/progressbar/right.png) no-repeat scroll right 1px transparent !important
  color: #0B0B0B
  float: left
  font-size: 12px
  height: 18px
  margin: -1px -12px 0 0
  padding: 2px 20px 0 0 !important
  text-decoration: none


.progress-bar a:hover
  text-decoration: underline


.progress-bar .selected a, .progress-bar .selected div
  color: #eee
  background: url(/assets/wellness/images/progressbar/hover_right.png) right top no-repeat !important
  margin-top: 0px
  padding-top: 1px !important
  height: 19px


