/* =Common forms styles
 * This section contains common form styles for all site, susch as wide-form,
 * norrow-form and etc.
 * @implementation
 * Form is center aligned block with shadow at topo, left and right sides.
 * There are different width forms. Basic implementaion idead based on two divs.
 * First div has background image with shadow (one shadow for each width), the
 * second div has background image with header (one header for each width).
 */

.form
  margin: 10px auto
  padding: 0 8px
  width: 960px
  background: url(/assets/wellness/images/forms/shadow_960.png) no-repeat 0 20px


.form-960
  width: 960px
  background: url(/assets/wellness/images/forms/shadow_960.png) no-repeat 0 20px


.form-850
  width: 850px
  background: url(/assets/wellness/images/forms/shadow_850.png) no-repeat 0 20px


.form-750
  width: 750px
  background: url(/assets/wellness/images/forms/shadow_750.png) no-repeat 0 20px


.form-440
  width: 440px
  background: url(/assets/wellness/images/forms/shadow_440.png) no-repeat 0 20px


.form-400
  width: 400px
  background: url(/assets/wellness/images/forms/shadow_400.png) no-repeat 0 20px



.form-360
  width: 360px
  background: url(/assets/wellness/images/forms/shadow_360.png) no-repeat 0 20px


.form-float
  float: left
  margin: 0 10px 10px 20px


.form-header
  height: 20px
  line-height: 20px
  background: url(/assets/wellness/images/forms/header_blue_960.png) no-repeat 0 0


.form-960 .form-header, .form-960 .form-header-blue
  background: url(/assets/wellness/images/forms/header_blue_960.png) no-repeat 0 0


.form-850 .form-header, .form-850 .form-header-blue
  background: url(/assets/wellness/images/forms/header_blue_850.png) no-repeat 0 0


.form-750 .form-header, .form-750 .form-header-blue
  background: url(/assets/wellness/images/forms/header_blue_750.png) no-repeat 0 0


.form-400 .form-header, .form-400 .form-header-blue
  background: url(/assets/wellness/images/forms/header_blue_400.png) no-repeat 0 0



.form-440 .form-header, .form-440 .form-header-blue
  background: url(/assets/wellness/images/forms/header_blue_440.png) no-repeat 0 0


.form-360 .form-header, .form-360 .form-header-blue
  background: url(/assets/wellness/images/forms/header_blue_360.png) no-repeat 0 0


.form-960 .form-header-gray
  background: url(/assets/wellness/images/forms/header_gray_960.png) no-repeat 0 0


.form-850 .form-header-gray
  background: url(/assets/wellness/images/forms/header_gray_850.png) no-repeat 0 0


.form-750 .form-header-gray
  background: url(/assets/wellness/images/forms/header_gray_750.png) no-repeat 0 0


.form-440 .form-header-gray
  background: url(/assets/wellness/images/forms/header_gray_440.png) no-repeat 0 0


.form-360 .form-header-gray
  background: url(/assets/wellness/images/forms/header_gray_360.png) no-repeat 0 0


