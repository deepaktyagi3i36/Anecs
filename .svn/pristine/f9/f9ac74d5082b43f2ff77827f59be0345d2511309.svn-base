/* =Common buttons styles
 * This section contains common buttons styles for all site, susch as
 * @todo
 */
.button
  display: inline
  border: 1px solid #E8E8E8
  text-decoration: none
  vertical-align: middle
  font-size: 11px


a.button:active
  background-position: bottom right


a.img-button
  display: inline
  margin-left: 5px
  text-decoration: none


a.img-button > img
  vertical-align: middle


input[type="submit"].blue-button, .blue-button
  background: url(/assets/wellness/images/primitives/btn_bg_blue.png) top repeat-x
  color: #ededed
