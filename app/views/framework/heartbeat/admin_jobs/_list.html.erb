<%= form_tag( {},{:id=>"list", :name=>"list"}) do %>
  <%= render :partial => "buttons" %>
  <table class="list-table">
    <col class="checkbox"/><col span=3 class="coldata"/>
    <col class="coldate"/><col class="actions"/>
    <tr>
      <th><%= checkable_field_header %></th>
      <th> <%= sortable_field_header t("activeview.columns.admin_job.name"), "admin_jobs.name" %> </th>
      <th> <%= sortable_field_header t("activeview.columns.admin_job.code"), "admin_jobs.code" %> </th>
      <th> <%= sortable_field_header t("activeview.columns.admin_job.job"), "admin_jobs.job" %> </th>
      <th> <%= sortable_field_header t("activeview.columns.admin_job.created_at"), "admin_jobs.created_at" %></th>
      <th> <%= t("activeview.columns.admin_job.actions") %> </th>
    </tr>
    <% @admin_jobs.each do |admin_job| %>
      <tr>
        <td><%= check_box_tag('ids[]', admin_job.id, false) %></td>
        <td><%= link_to admin_job.name, :action => "edit", :id =>admin_job %></td>
        <td><%= link_to admin_job.code, :action => "edit", :id =>admin_job %></td>
        <td><%= link_to admin_job.job, :action => "edit", :id =>admin_job %></td>
        <td><%= localize(admin_job.created_at, :format => :long) %></td>
        <td>
          <%=link_to(image_tag("buttons/update_object.png", {:alt => "Update", :title => "Update"}), :action => "edit", :id => admin_job) %>
          <%=link_to(image_tag("buttons/delete_object.png", {:alt => "Delete", :title => "Delete"}), :action => "destroy", :id => admin_job) %>
        </td>
      </tr>
    <% end %>
  </table>
  <%= render :partial => "buttons" %>
<%end%>
